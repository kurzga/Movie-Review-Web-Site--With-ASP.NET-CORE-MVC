<div class="modal video-modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                Giriş Yap & Kayıt Ol
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <section>
                <div class="modal-body">
                    <div class="w3_login_module">
                        <div class="module form-module">
                            <div class="toggle">
                                <i class="fa fa-times fa-pencil"></i>
                                <div class="tooltip">Bana Tıkla</div>
                            </div>
                            <div class="form">
                                <h3>Hesabına Giriş Yap</h3>
                                <form onsubmit="return login()">
                                    <input type="text" name="Username" id="username" placeholder="Kullanıcı Adı" required="">
                                    <input type="password" name="Password" id="pswd" placeholder="Şifre" required="">
                                    <input type="submit" value="Giriş Yap">
                                </form>

                                <script>
                                    // Kaydedilmiş kullanıcı bilgileri
                                    var users = [

                                        // Diğer kullanıcılar buraya eklenir
                                    ];

                                    function login() {
                                        var username = document.getElementById("username").value;
                                        var pswd = document.getElementById("pswd").value;

                                        // Kayıtlı kullanıcıları kontrol et
                                        for (var i = 0; i < users.length; i++) {
                                            if (username === users[i].username && pswd === users[i].password) {

                                                // Giriş başarılı olduğunda sayfaya yönlendir
                                                alert("Giriş başarılı ana sayfaya yönlendiriliyorsunuz.");
                                                window.location.href = "/Home/Index";
                                                return false; // Formu gönderme (sayfaya yönlendirildiğinden formu göndermemize gerek yok)
                                            }
                                        }

                                        // Kayıtlı kullanıcı yoksa veya hatalı girişse uyarı ver
                                        alert("Hatalı kullanıcı adı veya şifre.");
                                        return false; // Formu gönderme
                                    }
                                </script>
                            </div>

                            <div class="form">
                                <h3>Hesap Oluştur</h3>
                                <form onsubmit="return registerUser()">
                                    <input type="text" id="newUsername" placeholder="Kullanıcı Adı" required="">
                                    <input type="password" id="newPassword" placeholder="Şifre" required="">
                                    <input type="submit" value="Kayıt Ol">
                                </form>

                                <script>
                                    // Kullanıcıları saklamak için bir dizi oluştur
                                    var users = [];

                                    function registerUser() {
                                        var newUsername = document.getElementById("newUsername").value;
                                        var newPassword = document.getElementById("newPassword").value;

                                        // Yeni kullanıcıyı dizimize ekleyelim
                                        users.push({ username: newUsername, password: newPassword });

                                        // Başarıyla kaydedildiğini kullanıcıya bildir
                                        alert("Kullanıcı başarıyla kaydedildi!");

                                        // Formu temizle
                                        document.getElementById("newUsername").value = "";
                                        document.getElementById("newPassword").value = "";

                                        // Formun varsayılan gönderimini engelle
                                        return false;
                                    }
                                </script>
                            </div>
                            <div class="cta"><a href="#">Şifreni mi unuttun?</a></div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
